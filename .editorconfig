# top-most EditorConfig file
root = true

# Unix-style newlines with a newline ending every file
[*]
indent_style = space
end_of_line = crlf
insert_final_newline = true
charset = utf-8
trim_trailing_whitespace = false
insert_final_newline = false

[*.cs]
indent_size = 2

[*.{ts,tsx,js}]
indent_size = 2

[{package.json}]
indent_size = 2
